<h1>Please Log in</h1>

<form action="<%= session_url %>" method="post">
  <input type="hidden"
       name="authenticity_token"
       value="<%= form_authenticity_token %>">

  <label for="user_name">Username: </label>
  <input
    type="text"
    name="user[username]"
    id="user_name"
    >

    <br>

  <label for="password">Password: </label>
  <input
    type="password"
    name="user[password]"
    id="password"
    >

    <br>
    <input type="submit" name="" value="Log In">
</form>
