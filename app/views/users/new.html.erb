<h1>New User</h1>

<form action="/users" method="post">
  <input type="hidden"
       name="authenticity_token"
       value="<%= form_authenticity_token %>">

  <label for="user_name">Username: </label>
  <input
    type="text"
    name="user[username]"
    id="user_name"
    >


    <br>

  <label for="password">Password: </label>
  <input
    type="password"
    name="user[password]"
    id="password"
    >

  <br>
  <input type="submit" name="" value="Sign Up">
</form>
